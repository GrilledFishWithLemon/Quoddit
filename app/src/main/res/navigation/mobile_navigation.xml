<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.quoddit.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_searchUser"
        android:name="com.example.quoddit.SearchUserFragment"
        android:label="searchUser_fragment"
        tools:layout="@layout/fragment_search_user" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.quoddit.ProfileFragment"
        android:label="profile_fragment"
        tools:layout="@layout/profile_fragment" />



</navigation>